{
  "name": "journaling-server-node",
  "version": "1.0.0",
  "description": "",
  "repository": {},
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only index.ts",
    "typecheck": "tsc --incremental",
    "generate:types:gql": "graphql-codegen --config codegen.yml",
    "generate:types:gql:watch": "npm run generate:types:gql -- --watch \"src/schema.ts\"",
    "repl": "node -r ./scripts/repl.js",
    "build:ts": "npm run generate:types:gql && tsc",
    "build:docker": "docker build .",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.0",
    "@graphql-codegen/introspection": "1.17.0",
    "@graphql-codegen/typescript": "1.17.0",
    "@graphql-codegen/typescript-resolvers": "1.17.0",
    "@types/aws-lambda": "^8.10.59",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/yup": "^0.29.7",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "apollo-server": "^2.16.0",
    "apollo-server-express": "^2.16.0",
    "aws-sdk": "^2.728.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "raw-body": "^2.4.1",
    "yup": "^0.29.3"
  }
}
