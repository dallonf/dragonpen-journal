{
  "name": "journaling-server-node",
  "version": "1.0.0",
  "description": "",
  "repository": {},
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only index.ts",
    "typecheck": "tsc --incremental",
    "generate:types:gql": "graphql-codegen --config codegen.yml",
    "generate:types:gql:watch": "npm run generate:types:gql -- --watch \"src/schema.ts\"",
    "repl": "node -r ./scripts/repl.js",
    "build:ts": "npm run generate:types:gql && tsc",
    "build:docker": "docker build .",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.5",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/aws-lambda": "^8.10.101",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/yup": "^0.29.14",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.0",
    "apollo-server": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "aws-sdk": "^2.1191.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "knex": "^2.2.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "raw-body": "^2.5.1",
    "yup": "^0.32.11"
  }
}
