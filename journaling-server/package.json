{
  "name": "journaling-server-node",
  "version": "1.0.0",
  "description": "",
  "repository": {},
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only index.ts",
    "typecheck": "tsc --incremental",
    "generate:types:gql": "graphql-codegen --config codegen.yml",
    "generate:types:gql:watch": "npm run generate:types:gql -- --watch \"src/schema.ts\"",
    "repl": "node -r ./scripts/repl.js",
    "build:ts": "npm run generate:types:gql && tsc",
    "build:docker": "docker build .",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-resolvers": "^1.19.0",
    "@types/aws-lambda": "^8.10.59",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/yup": "^0.29.7",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "apollo-server": "^2.23.0",
    "apollo-server-express": "^2.23.0",
    "aws-sdk": "^2.728.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "raw-body": "^2.4.1",
    "yup": "^0.29.3"
  }
}
